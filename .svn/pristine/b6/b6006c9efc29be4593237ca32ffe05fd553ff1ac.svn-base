package com.wcs.tih.transaction.controller.vo;

import java.io.Serializable;

import com.wcs.common.controller.helper.IdModel;
import com.wcs.tih.model.WfMailConfig;

public class MailConfigVo extends IdModel implements Serializable {

	private static final long serialVersionUID = 5315344867679205830L;
	private WfMailConfig wfMailConfig;
	private boolean emailFlag;
	private boolean sysNoticeFlag;

	public WfMailConfig getWfMailConfig() {
		return wfMailConfig;
	}

	public void setWfMailConfig(WfMailConfig wfMailConfig) {
		this.wfMailConfig = wfMailConfig;
	}

	public boolean isEmailFlag() {
		return emailFlag;
	}

	public void setEmailFlag(boolean emailFlag) {
		this.emailFlag = emailFlag;
	}

	public boolean isSysNoticeFlag() {
		return sysNoticeFlag;
	}

	public void setSysNoticeFlag(boolean sysNoticeFlag) {
		this.sysNoticeFlag = sysNoticeFlag;
	}
}
